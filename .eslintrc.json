{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["jest"],
  "parser": "babel-eslint",
  "rules": {
    "no-param-reassign": ["error", { "props": false }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "prettier/prettier": ["error", { "singleQuote": true, "endOfLine": "auto" }]
  }
}
